name: dockercompose40707640440000624
services:
  propostadb:
    container_name: propostadbrover
    environment:
      MONGO_INITDB_ROOT_PASSWORD: numsey
      MONGO_INITDB_ROOT_USERNAME: macoratti
    image: mongo
    networks:
      default: null
    ports:
      - mode: ingress
        target: 27018
        published: "27021"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: mongo_propostadbrover_data
        target: /data/db
        volume: {}
networks:
  default:
    name: dockercompose40707640440000624_default
volumes:
  mongo_propostadbrover_data:
    name: dockercompose40707640440000624_mongo_propostadbrover_data